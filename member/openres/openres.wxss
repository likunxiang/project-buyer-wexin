page{background: #fff;}
.header{text-align: center;background: #c10000;height: 214rpx;color: #fff;font-size: 30rpx;letter-spacing:4rpx;}
.header .clear{display: inline-block;margin: 0 auto;padding-top: 46rpx;margin-bottom: 20rpx;font-weight: 600;font-size: 36rpx;}
.header image{width: 49rpx;height: 49rpx;margin-right: 20rpx;margin-left: -20rpx;}
.content{color: #595959;text-align: center;padding: 40rpx 74rpx;font-size: 28rpx;}
.content .tips{font-weight: 600;}
.content .clear{margin-top: 91rpx;}
.typeNav{color: #636363;text-align: center;width: 180rpx;height: 88rpx;line-height: 88rpx;border: 1px solid #979797;margin-right: 28rpx;margin-bottom: 52rpx;border-radius: 8rpx;}
.typeNav:nth-child(3n+3){margin-right: 0;}
.content .active{background: #c10000;border: 1px solid #c10000;color: #fff;}
.inteBtn{color: #fff;font-size: 30rpx;font-weight: 600;text-align: center;position: absolute;width: 308rpx;height: 78rpx;line-height: 78rpx;left: 50%;bottom: 52rpx;transform: translateX(-50%);background: #c10000;}