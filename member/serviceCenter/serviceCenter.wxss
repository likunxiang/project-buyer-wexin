	height: 100%;
}
.page {
	min-height: 100%;
}
.copy-btn {
	width:75rpx;
	height:41rpx;
	line-height: 39rpx;
	text-align: center;
	border:1rpx solid rgba(153,153,153,1);
	border-radius:4rpx;
}
.nav-silde {
	width: 165rpx;
	background: #F4F4F4;
}
.nav-box {
	width: 100%;
	height: 92rpx;
	line-height: 92rpx;
	text-align: center;
	font-size: 30rpx;
	color: #333333;
}
.nav-box-active {
	background: #fff;
}
